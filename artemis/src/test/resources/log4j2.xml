<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout
                pattern="%d{HH:mm:ss.SSS} [%-5level] - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="stdout" level="info">
            <AppenderRef ref="console"/>
        </Logger>
    </Loggers>
</Configuration>