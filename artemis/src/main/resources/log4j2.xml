<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout
                pattern="[%-5level] - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="all" additivity="false">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>